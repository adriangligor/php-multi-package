{
    "name": "adriangligor/mod-a",
    "type": "project",
    "require": {
        "php": "^7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "adriangligor/mod-shared": "@dev",
        "vanilla/garden-cli": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/"
        }
    },
    "repositories": {
        "mod-shared": {
            "type": "path",
            "url": "../mod-shared"
        }
    }
}
